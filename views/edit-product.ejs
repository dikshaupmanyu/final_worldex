                           <div class="row-fluid">
                      <div class="span12">
                        <!-- BEGIN SAMPLE FORM PORTLET-->   
                        
                        <div class="portlet box blue tabbable">
                            <div class="portlet-title">
                                <div class="caption">
                                    <span class="hidden-480">Add Product</span>
                                </div>
                            </div>

                            <div class="portlet-body form">
                                <div class="tabbable portlet-tabs">
                                    
                                    <div class="tab-content">
                                        <div class="tab-pane active" id="portlet_tab1">
                                  <% for (var i = 0; i < data.length; i++) if(data[i]._id == productid) { %>    
                                         <form class="form-horizontal" method="post" action="/updateproduct">
                                               
                                               <input type="text" name="_id" class="form-control"   value="<%= data[i]._id %>" style="display: none;" required>

                                                            <div class="control-group">
                                                                <label class="control-label">Product Name</label>
                                                                <div class="controls">
                                                                    <input type="text" name="productname" class="form-control"  placeholder="Product Name" value="<%= data[i].productname %>" required>
                                                                </div>
                                                            </div>

                                                             <div class="control-group">
                                                                <label class="control-label">Product Name (Chineese)</label>
                                                                <div class="controls">
                                                                    <input type="text" name="productname_chineese" class="form-control" value="<%= data[i].productname_chineese %>"  placeholder="Product Name (Chineese)" required>
                                                                </div>
                                                            </div>


                                                            <div class="control-group">
                                                                <label class="control-label">Product Price</label>
                                                                <div class="controls">
                                                                    <input type="text" name="productprice" class="form-control"  placeholder="Product Price" value="<%= data[i].productprice %>" required>
                                                                </div>
                                                            </div>
                                                            <div class="control-group">
                                                                <label class="control-label">Product Description</label>
                                                                <div class="controls">
                                                                     <textarea type="text" name="productdescr" class="form-control" placeholder="Product Description" required><%= data[i].productdescr %></textarea>
                                                                </div>
                                                            </div>

                                                             <div class="control-group">
                                                                <label class="control-label">Product Full Description</label>
                                                                <div class="controls">
                                                                     <textarea type="text" name="productfulldescr" class="form-control"  required><%= data[i].productfulldescr %></textarea>
                                                                </div>
                                                            </div>

                                                            <div class="control-group">
                                                                <label class="control-label">Product Category</label>
                                                                <div class="controls">
                                                                
                                                                     <select name="productcategory" class="form-control">
                                                                    
                                                                         <option value="Frequently Bought Formulas">Frequently Bought Formulas</option>
                                                                         <option value="Single Herbs">Single Herbs</option>
                                                                         <option value="Classic Formulas">Classic Formulas</option>
                                                                     </select>

                                                                </div>
                                                            </div>

                                                            <div class="control-group">
                                                                <label class="control-label">Pack Size</label>
                                                                <div class="controls">
                                                                     <input type="text" name="prosize" class="form-control" value="<%= data[i].prosize %>" placeholder="Pack Size" required>
                                                                </div>
                                                            </div>

                                                                <div class="control-group">
                                                                <label class="control-label">Additional Information</label>
                                                                <div class="controls">
                                                                     <textarea type="text" name="addInfo" class="form-control" required><%= data[i].addInfo %></textarea>
                                                                </div>
                                                            </div>

                                                            <div class="control-group">
                                                                <label class="control-label">Product Type</label>
                                                                <div class="controls">
                                                                
                                                                     <select name="producttype" class="form-control">
                                                                    
                                                                         <option value="User">User</option>
                                                                         <option value="Practicioner">Practicioner</option>
                                                                        
                                                                     </select>

                                                                </div>
                                                            </div>

                                                            <input type="hidden" id="datetime">

                                                             <div class="row-fluid">
                                                             <div class="span4"></div>
                                                             <div class="span3">
                                                            <button class="btn theme-btn" type="submit"> <i class="fa fa-check"></i>Product Add</button>
                                                             </div>
                                                             <div class="span3">
                                                             <a href="account"><button style="text-decoration: none;outline: 0 none;" class="btn theme-btn"><i class="fa fa-arrow"></i> Cancel</button></a>
                                                                </div>
                                                              <div class="span2"></div>  
                                                                </div>


                                                        </form>

                                        <% } %>
                                      

                                          
                                        </div>
                                       
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- END SAMPLE FORM PORTLET-->



                    </div>
                </div>
                    
                    </div>
                </div>
                </div>
                </div>

