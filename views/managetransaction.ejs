                          <div class="span9" style="height: 800px;">
                        <!-- BEGIN PAGE HEADER-->
                        <div class="row-fluid">
                            <div class="span12">
                            
                                <!-- BEGIN PAGE TITLE & BREADCRUMB-->
                            <h6 class="page-title" style="font-size: 18px;">
                                  <%= user.firstName %>'s Profile !!!!
                                </h6>
                            </div>
                        </div>
                        <!-- END PAGE HEADER-->
                       


                           <div class="row-fluid">
                      <div class="span12">
                        <!-- BEGIN SAMPLE FORM PORTLET-->   
                        
                        <div class="portlet box blue tabbable">
                            <div class="portlet-title">
                                <div class="caption">
                                    <span class="hidden-480">Order Managment</span>
                                </div>
                            </div>

                            <div class="portlet-body form">
                                <div class="tabbable portlet-tabs">
                                    
                                    <div class="tab-content">
                                        <div class="tab-pane active" id="portlet_tab1">
                                        <form class="form-horizontal" style="overflow: auto;">
                                             <table class="table table-striped table-bordered" id="example">
                                  <thead>
                                    <tr>
                                     <th>Order Id</th>
                                      <th>Name</th>
                                  
                                    <th>Payment Id</th>
                                      <th>Amount Charge</th>
                                      <th>Transaction Key</th>
                                      <th>Status</th>
                                      <th>Date</th>
                                      <th>Card Detail</th>
                                     <th>Apply Refund</th>
                                      <th>Action</th>
                                    </tr>
                                  </thead>
                                 
                                  <tbody>
                                     <% for (var i = 0; i < data.length; i++) { %>
                                    <tr>

                                         <td><%= data[i].orderid %> </td>
                                         <% if(data[i].userid == 'guest') { %>
                                            <td>Guest</td>
                                         <% } else { %>
                                        <% for (var j = 0; j < datauser.length; j++) if(datauser[j]._id == data[i].userid){ %>
                                            <td><%= datauser[j].firstName %> </td>
                                           <% } %>
                                         <% } %>
                                         <td><%= data[i].paymentId %></td>
                                         <td><%= data[i].amount*0.01 %></td>
                                         <td>$<%= data[i].balance_transaction %></td>
                                         <td><%= data[i].paid_status %></td>
                                         <td><%= data[i].datetime %></td>
                                         <td><%= data[i].sourceId %></td>
                            
                                        <td><a href="refund?order_id=<%= data[i].orderid %>"><i class="fa fa-share" aria-hidden="true" style="font-size: 20px;" ></i></a></td>
                                       <td><a href="deletetransaction?transaction_id=<%= data[i]._id %>"><i class="fa fa-trash-o" aria-hidden="true" style = " font-size: 20px"></i></a></td>
                                     
                                     
                                    </tr>
                                     <% } %>
                                  </tbody>
                                </table>
                                        </form>
                                        
                                      

                                          
                                        </div>
                                       
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- END SAMPLE FORM PORTLET-->



                    </div>
                </div>
                    
                    </div>
                </div>
                </div>
                </div>

