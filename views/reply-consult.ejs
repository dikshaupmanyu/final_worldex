                            <div class="row-fluid">
                                <div class="span12">
                                    <!-- BEGIN SAMPLE FORM PORTLET-->   
                                    <div class="flash-message" ng-if="flash">
                                                <div class="{{'alert alert-' + flash.type}}" ng-bind="flash.message"></div>
                                     </div>
                                    <div class="portlet box blue tabbable">
                                        <div class="portlet-title">
                                            <div class="caption">
                                             
                                                <span class="hidden-480">Orders List</span>
                                            </div>
                                        </div>

                                         <div class="portlet-body form">
                                            <div class="tabbable portlet-tabs" id="portlet_tab1">

                                            
                                                        <div class="control-group" style="text-align: center;">
                                                         <h4><b> Your Order Id - <%= consultid %> </b></h4>

                                                        </div>         
                                                   <% for (var i = 0; i < data.length; i++) if(data[i]._id == consultid) { %>       
                                                          <form class="form-horizontal" style="height: 300px;">        

                                                          <% for (var j = 0; j < datauser.length; j++) if(datauser[j]._id == data[i].user_id) { %>                  
                                                                 <div class="control-group">
                                                                    <label class="control-label"> Name</label>
                                                                    <div class="controls">


                                                                       <input type="text" id="firstName" name="firstName" class="form-control" value="<%= datauser[j].firstName %>" readonly  />
                                                                    </div>
                                                                   
                                                                </div>

                                                            <% } %>    
                                                                <div class="control-group">
                                                                    <label class="control-label">Problem</label>
                                                                    <div class="controls">

                                                                        <input type="text" name="problem" class="form-control" value="<%= data[i].problem %>" readonly />


                                                                    </div>
                                                                </div>
                                                                <div class="control-group">
                                                                    <label class="control-label">Problem Area</label>
                                                                    <div class="controls">
                                                     <input type="text" name="problemarea" value="<%= data[i].problemarea %>" class="form-control" readonly />
                                                                    </div>
                                                                </div>

                                                              

                                                                <div class="control-group">
                                                                    <label class="control-label">Problem Detail</label>
                                                                    <div class="controls">
                                                                    <textarea  class="form-control"  rows="3" readonly><%= data[i].problemdetail %></textarea>
                                                                    </div>
                                                                </div>

                                  </form>

                                  <% } %>
                                  
                                  
                                 

                                 <form action="reply-consult-insert" class="form-horizontal" method="post" enctype="multipart/form-data" style="height: 280px;">
                                          

                                                        <div class="control-group" style="text-align: center;">
                                                         <h4><b> Doctor's Reply </b></h4>

                                                        </div>         


                                              <div class="flash-message" ng-if="flash">
          <div class="{{'alert alert-' + flash.type}}" ng-bind="flash.message"></div>
         </div>


                                          

                                                       <div class="control-group">
                                                                <label class="control-label">Product Name</label>
                                                                <div class="controls">
                                                                <select multiple="multiple" class="js-example-basic-single" name="product_id[]" required="required" style="width:80%;">
                                                                <option>Select Product Name</option>
                                                                 <% for (var k = 0; k < dataproduct.length; k++) { %> 
                                                                  <option value="<%= dataproduct[k]._id %>">
                                                                    <%= dataproduct[k].productname %>
                                                                  </option>
                                                                  <% } %>
                                                                </select>
                                                                    
                                                                </div>
                                                            </div>


                                                                <div class="control-group">
                                                                <label class="control-label">Product Description</label>
                                                                <div class="controls">
                                                                     <textarea type="text" name="comment" class="form-control" required></textarea>
                                                                </div>
                                                            </div>


<input type="text" style="display: none;" name="consult_status" value="Answered">
<input type="text" style="display: none;"  name="consult_id" value="<%= consultid %>">



                                                              

                                                                   <div class="row-fluid">
                                                             <div class="span4"></div>
                                                             <div class="span3">
                                                               <button type="submit" class="btn theme-btn"  style="display: block;margin: 0 auto;"> <i class="fa fa-check"></i>Consult Reply</button>
                                                             </div>
                                                             <div class="span3">
                                                             <a href="account"><button style="text-decoration: none;outline: 0 none;" class="btn theme-btn"><i class="fa fa-arrow"></i> Cancel</button></a>
                                                                </div>
                                                              <div class="span2"></div>  
                                                                </div>

                                                           
                            
                                    

                                      </form>
                                   
                                        </div>

                                      
